{
  "name": "rutgers-golf-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preflight": "node scripts/preflight-check.js",
    "predev": "npm run preflight",
    "dev": "node scripts/dev-server.js start",
    "dev:stop": "node scripts/dev-server.js stop",
    "dev:status": "node scripts/dev-server.js status",
    "dev:restart": "npm run dev:stop && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "node scripts/setup-db.js",
    "validate": "npm run preflight && npm run lint && npm run type-check && npm run format:check",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-big-calendar": "^1.16.2",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.7",
    "next": "14.2.30",
    "openai": "^5.10.2",
    "react": "^18",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.30",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
